package pack2;

import org.testng.Assert;
import org.testng.annotations.Test;

public class Test3 {
  @Test
  public void add() {
	  int a = 45;
	  int b = 20;
	  int c = a+b;
	  Assert.assertEquals(c, 65);
  }
  
  @Test
  public void sub() {
	  int a = 45;
	  int b = 20;
	  int c = a-b;
	  Assert.assertEquals(c, a-b);
  }
  
  @Test
  public void mul() {
	  int a = 45;
	  int b = 20;
	  int c = a*b;
	  Assert.assertEquals(c, a*b);
  }
  
  @Test
  public void div() {
	  int a = 45;
	  int b = 20;
	  int c = a/b;
	  Assert.assertEquals(c, a/b);
  }
}
